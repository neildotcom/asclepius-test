# Asclepius Environment Configuration
# Copy this file to .env.local and update with your deployed values

# WebSocket endpoint from CDK deployment
# Get this from: aws cloudformation describe-stacks --stack-name Asclepius-dev --query 'Stacks[0].Outputs[?OutputKey==`WebSocketEndpoint`].OutputValue' --output text
REACT_APP_WEBSOCKET_ENDPOINT=wss://your-alb-dns-name.us-east-1.elb.amazonaws.com

# Optional: Custom domain if you configured one
# REACT_APP_WEBSOCKET_ENDPOINT=wss://api.yourdomain.com

# AWS Region (should match your CDK deployment)
REACT_APP_AWS_REGION=us-east-1

# Stage/Environment
REACT_APP_STAGE=dev

# DynamoDB Table Names (from CDK deployment)
# Get these from CDK stack outputs
REACT_APP_VISIT_DATA_TABLE=asclepius-visit-data-dev
REACT_APP_PATIENT_TABLE=asclepius-patient-dev
REACT_APP_VISIT_TABLE=asclepius-visit-dev
REACT_APP_TRANSCRIPT_TABLE=asclepius-transcript-dev
